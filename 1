trigger:
  branches:
    include:
      - master

schedules:
  - cron: "*/5 * * * *" # Run every 5 minutes
    displayName: "Run every 5 minutes"
    branches:
      include:
        - master

pool:
  vmImage: 'ubuntu-latest'

name: Ansible-Syseng

stages:
  - stage: Stage95
    displayName: "Run main95.py"
    jobs:
      - job: Job95
        displayName: "Run main95.py Script"
        steps:
          - script: |
              python3 Unity_API_CPU/main95.py
            displayName: "Execute main95.py"
            env:
              SITE_USER: $(SITE_USER) # Environment variables
              SITE_PASSWORD: $(SITE_PASSWORD)
              fwpass: $(fwpass)

  - stage: Stage96
    displayName: "Run main96.py"
    dependsOn: Stage95 # Runs only after Stage95 is complete
    jobs:
      - job: Job96
        displayName: "Run main96.py Script"
        steps:
          - script: |
              python3 Unity_API_CPU/main96.py
            displayName: "Execute main96.py"
            env:
              SITE_USER: $(SITE_USER) # Environment variables
              SITE_PASSWORD: $(SITE_PASSWORD)
              fwpass: $(fwpass)
