
<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="ProxyAPICalls" stopProcessing="true">
          <match url="^api/(.*)" />
          <action type="Rewrite" url="http://10.7.32.134:3000/api/{R:1}" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
