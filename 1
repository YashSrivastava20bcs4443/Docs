npm install -g concurrently

package.json:

{
  "name": "contact-center-dashboard",
  "version": "1.0.0",
  "scripts": {
    "start:backend": "node script.js",
    "start:frontend": "cd client && npm start",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "dependencies": {
    "concurrently": "^8.2.0"
  }
}

npm start
