SELECT 
    v.DisplayName AS 'ComputerName',
    p.ObjectName AS 'ObjectType',
    p.CounterName AS 'Counter',
    p.InstanceName AS 'Instance',
    p.SampleValue AS 'PerformanceValue',
    p.TimeSampled AS 'TimeStamp'
FROM 
    dbo.Perf AS p
JOIN 
    dbo.vManagedEntity AS v
    ON p.ManagedEntityID = v.ManagedEntityID
ORDER BY 
    p.TimeSampled DESC;
