trigger:
  none

schedules:
  - cron: "0 * * * *"  # Har ghante ke zero minute par chalega
    displayName: Run hourly from 5 PM IST onwards
    branches:
      include:
        - master
    always: true
    startTime: "2025-01-07T11:30:00Z"  # 5 PM IST (11:30 AM UTC)

pool:
  name: 'Ansible-Syseng'

stages:
  - stage: Stage1
    displayName: "Run main.py"
    jobs:
      - job: Job9596
        displayName: "Run main.py Script"
        steps:
          - script: |
              python3 Unity_API_CPU/main95.py
              python3 Unity_API_CPU/main96.py
            displayName: "Execute Python Scripts"
