import pandas as pd
import streamlit as st

st.set_page_config(
    page_title="Log File Data",
    page_icon="âœ…",
    layout="wide",
)

# Function to read and parse the log file
@st.experimental_memo
def load_data() -> pd.DataFrame:
    with open('/mnt/data/console-output-CLI-Console-(1).txt', 'r') as file:
        logs = file.readlines()
    
    # Extract fields from log entries
    data = []
    for log in logs:
        fields = {}
        parts = log.split()
        for part in parts:
            if "=" in part:
                key, value = part.split("=", 1)
                fields[key] = value.strip('"')
        data.append(fields)
    
    return pd.DataFrame(data)

# Load the data
df = load_data()

# Display the DataFrame
st.title("Log File Data")
st.write(df)
