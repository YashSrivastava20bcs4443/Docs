def print_content(data):
    try:
        system_content = data['entries'][0]['content']
        print(json.dumps(system_content, indent=4))
    except (KeyError, IndexError) as e:
        print("Error printing system content:", e)

# Call this function to print the system content
print_content(system_info)

def print_full_response(data):
    print("Full Response:")
    print(json.dumps(data, indent=4))

# Call this function to print the full response
print_full_response(system_info)
